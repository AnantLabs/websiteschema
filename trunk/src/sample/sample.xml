<FBNetwork >
    <FB Name="TEST" Type="websiteschema.fb.Test" />
    <FB Name="LAUNCHER" Type="websiteschema.fb.Launcher" />
    <EventConnections>
        <Connection Source="LAUNCHER.INIT" Destination="TEST.INIT" />
        <Connection Source="LAUNCHER.STOP" Destination="TEST.STOP" />
        <Connection Source="TEST.INIT" Destination="LAUNCHER.STOP" />
    </EventConnections>
    <DataConnections>
        <Connection Source="LAUNCHER.OUT" Destination="TEST.IN" />
    </DataConnections>
</FBNetwork>
